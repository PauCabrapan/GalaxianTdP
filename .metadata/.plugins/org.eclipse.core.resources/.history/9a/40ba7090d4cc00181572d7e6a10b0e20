package personajes;
import mapa.*;

public abstract class Personajes extends EntidadMovimiento {
	
	protected int fuerza_kamikaze;
	
	public Personajes(Celda c){
		super(c);
		velocidad=10;
	}
	
	protected void mover(int dir){
		Celda next = this.pos.getVecina(dir);
		if(next != null){
			this.pos = next;
			this.grafico.mover(dir);
		}
	}

}
